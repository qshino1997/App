<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@clothe) do |f| %>
    <%= render 'shared/thonbaoloi' %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :gia %>
      <%= f.text_field :gia %>

      <%= f.label :soluong %>
      <%= f.text_field :soluong %>

      <span class="picture">
      <%= f.file_field :picture %>
      </span>

      <%= f.submit "Them san pham", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
